@model Nop.Web.Areas.Admin.Models.QrCodes.SunworldQrCodeModel

@{
    //page title
    ViewBag.PageTitle = T("Admin.Catalog.Categories.EditQrCodeDetailsDetails").Text;
    Html.SetActiveMenuItemSystemName("ManageQrCode");
}

<div class="content">
    <form asp-controller="ManageQrCode" asp-action="Edit">
        <div class="content-header clearfix">
            <h1 class="pull-left">
                @T("Admin.Catalog.Categories.EditQrCodeDetailsDetails") - @Model.BarCode
                <small>
                    <i class="fa fa-arrow-circle-left"></i>
                    <a asp-action="Index">@T("Admin.ManageQrCode.ManageQrCodes.BackToList")</a>
                </small>
            </h1>
            <div class="pull-right">
                <button type="submit" name="save" class="btn bg-blue">
                    <i class="fa fa-floppy-o"></i>
                    @T("Admin.Common.Save")
                </button>
                <span id="category-delete" class="btn bg-red">
                    <i class="fa fa-trash-o"></i>
                    @T("Admin.Common.Delete")
                </span>
            </div>
        </div>
        @await Html.PartialAsync("_CreateOrUpdate", Model)
    </form>
</div>
